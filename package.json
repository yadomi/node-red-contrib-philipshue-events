  },
  "engines": {
    "node": ">=12.0.0"
  },
  "node-red": {
    "version": ">=2.0.0",
    "nodes": {
      "philipshue-events-config": "lib/philipshue-events-config/philipshue-events-config.js",
      "philipshue-events": "lib/philipshue-events/philipshue-events.js"
    }
  },
  "files": [
    "lib/**/*",
    "examples/**/*"
  ],
  "keywords": [
